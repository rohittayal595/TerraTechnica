<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/splash_back">

    <com.github.jorgecastillo.FillableLoader
        android:id="@+id/fillableLoader"
        android:layout_width="280dp"
        android:layout_height="280dp"
        android:layout_centerInParent="true"
        app:fl_clippingTransform="waves"
        app:fl_fillColor="#10663b"
        app:fl_fillDuration="1500"
        app:fl_originalHeight="512"
        app:fl_originalWidth="512"
        app:fl_strokeColor="#10663b"
        app:fl_strokeDrawingDuration="1500"
        app:fl_strokeWidth="2dp" />
</RelativeLayout>